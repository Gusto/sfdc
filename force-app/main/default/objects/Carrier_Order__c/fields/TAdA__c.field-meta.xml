<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TAdA__c</fullName>
    <description>This field is marked as TRUE if the Carrier associated to the Carrier Order has Partner Type = TAdA</description>
    <formula>if( AND(NOT( ISBLANK( Carrier__c)),ISPICKVAL(Carrier__r.Partner_Type__c, &apos;TAdA&apos;)),true,false )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>TAdA</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
