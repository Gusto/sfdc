<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Closed__c</fullName>
    <formula>OR(
    ISPICKVAL(Implementation_Stage__c,&apos;Approved&apos;),
    ISPICKVAL(Implementation_Stage__c,&apos;Canceled&apos;),
    ISPICKVAL(Implementation_Stage__c,&apos;Closed Lost&apos;),
    ISPICKVAL(Implementation_Stage__c,&apos;Closed Admin&apos;)
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Closed</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
