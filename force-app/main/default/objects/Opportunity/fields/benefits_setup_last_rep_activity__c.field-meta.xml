<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>benefits_setup_last_rep_activity__c</fullName>
    <description>Difference in last rep activity on the account less last benefits step updated</description>
    <externalId>false</externalId>
    <formula>DATETIMEVALUE(Account.Last_Activity_Date__c) -  DATETIMEVALUE(Benefits_Setup_Last_Updated__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>benefits setup &gt; last rep activity</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
