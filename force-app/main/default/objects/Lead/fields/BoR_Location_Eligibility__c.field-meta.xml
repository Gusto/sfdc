<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>BoR_Location_Eligibility__c</fullName>
    <description>Determines eligibility based on the supported BoR states from Lead States, Address, and Inferred State Region.</description>
    <inlineHelpText>Supported BoR states: CA, CO, FL, IL, MA, NJ, NY, OH, TX, WA.
Returns &apos;Eligible&apos; if at least one state is supported
Returns &apos;Ineligible&apos; if no states are supported
Returns &apos;Unknown&apos; if states are blank</inlineHelpText>
    <label>BoR Location Eligibility</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Picklist</type>
    <valueSet>
        <restricted>true</restricted>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>Unknown</fullName>
                <default>true</default>
                <label>Unknown</label>
            </value>
            <value>
                <fullName>Eligible</fullName>
                <default>false</default>
                <label>Eligible</label>
            </value>
            <value>
                <fullName>Ineligible</fullName>
                <default>false</default>
                <label>Ineligible</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>
