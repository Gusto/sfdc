<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Negative_employees</fullName>
    <active>true</active>
    <errorConditionFormula>NumberOfEmployees != null &amp;&amp; NumberOfEmployees &lt; 0</errorConditionFormula>
    <errorDisplayField>NumberOfEmployees</errorDisplayField>
    <errorMessage>Employees can never be less than 0.</errorMessage>
</ValidationRule>
