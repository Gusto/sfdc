<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PEO_Detail_Required</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
ISPICKVAL( PEO__c, &quot;Other&quot;),
ISBLANK( PEO_Detail__c)
)</errorConditionFormula>
    <errorMessage>PEO Detail is required if PEO is &apos;Other&apos;</errorMessage>
</ValidationRule>
