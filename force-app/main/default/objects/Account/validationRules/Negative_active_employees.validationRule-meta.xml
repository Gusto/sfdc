<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Negative_active_employees</fullName>
    <active>true</active>
    <errorConditionFormula>EE_Active_Count_Panda__c != null &amp;&amp; EE_Active_Count_Panda__c &lt; 0</errorConditionFormula>
    <errorDisplayField>EE_Active_Count_Panda__c</errorDisplayField>
    <errorMessage>Active employees can never be less than 0.</errorMessage>
</ValidationRule>
