<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Unit_price_must_equal_list_price</fullName>
    <active>true</active>
    <errorConditionFormula>UnitPrice != ListPrice</errorConditionFormula>
    <errorDisplayField>ListPrice</errorDisplayField>
    <errorMessage>The price cannot be changed.</errorMessage>
</ValidationRule>
