<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Days_Until_Follow_Up__c</fullName>
    <description>Follow_Up_Date__c - TODAY() = Number of days until follow up. Field is used to improve Engagements workflow.</description>
    <externalId>false</externalId>
    <formula>IF( Follow_Up_Date__c - TODAY() &gt; 0, Follow_Up_Date__c - TODAY(),0)</formula>
    <label>Days Until Follow Up</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
