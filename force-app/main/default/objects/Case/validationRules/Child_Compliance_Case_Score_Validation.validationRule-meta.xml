<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Child_Compliance_Case_Score_Validation</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
    NOT(ISBLANK(ParentId)),
    ISCHANGED(Compliance_Score__c)
)</errorConditionFormula>
    <errorDisplayField>Compliance_Score__c</errorDisplayField>
    <errorMessage>Compliance Score cannot be updated for child case</errorMessage>
</ValidationRule>
