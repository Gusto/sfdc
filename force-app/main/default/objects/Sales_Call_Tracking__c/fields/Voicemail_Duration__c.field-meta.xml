<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Voicemail_Duration__c</fullName>
    <description>Captures voicemail duration - in case voicemail was send - from PureCloud.</description>
    <externalId>false</externalId>
    <formula>(Voicemail_End_Time__c - Voicemail_Start_Time__c)*24*60</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Voicemail Duration</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
