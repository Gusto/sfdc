<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Has_Run_Payroll__c</fullName>
    <description>Used by Benefit Transfer team</description>
    <formula>AND (
NOT(ISBLANK(Opportunity__r.Account.Joined_Date_Panda__c)),
NOT(ISPICKVAL(Opportunity__r.Account.ZP_Company_Overall_Status__c, &quot;Suspended&quot;)))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Has Run Payroll</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
