<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ZP_Adjustment_ID_must_be_a_number</fullName>
    <active>true</active>
    <description>Due to rules in ZP Adjustment for only updating with newer records,</description>
    <errorConditionFormula>!ISBLANK( ZP_Adjustment_Id__c ) &amp;&amp; !ISNUMBER(ZP_Adjustment_Id__c)</errorConditionFormula>
    <errorDisplayField>ZP_Adjustment_Id__c</errorDisplayField>
    <errorMessage>ZP Adjustment Id must be a number</errorMessage>
</ValidationRule>
