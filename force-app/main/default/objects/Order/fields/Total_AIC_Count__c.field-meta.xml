<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Total_AIC_Count__c</fullName>
    <description>This field holds the summation on three count fields &quot;Contractor_Count__c&quot;, &quot;Inactive_Employee_Count__c &quot;,&quot;Active_Employee_Count__c&quot;</description>
    <externalId>false</externalId>
    <formula>Contractor_Count__c  +  Inactive_Employee_Count__c  +  Active_Employee_Count__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Total AIC Count</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
