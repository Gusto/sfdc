        <rules>
            <name>Auto_Solve</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>objCase.Automation_Message__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Auto Solve Email Alert</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>objOldCase.Automation_Message__c</leftValueReference>
                <operator>NotEqualTo</operator>
