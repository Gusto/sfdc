                    <elementReference>objCase.Automation_Message__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>objCase.Automation_Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Started</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>objOldCase.Automation_Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>objCase.Automation_Status__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Notify_customers_40_hours_after_the_case_is_responded</targetReference>
