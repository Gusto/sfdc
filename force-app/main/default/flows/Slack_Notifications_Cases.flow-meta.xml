        </rules>
        <rules>
            <name>Complex_Case</name>
            <conditionLogic>1 AND 2 AND 3 AND (4 OR 5)</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Handle_Time_Seconds_New__c</leftValueReference>
                <operator>GreaterThanOrEqualTo</operator>
