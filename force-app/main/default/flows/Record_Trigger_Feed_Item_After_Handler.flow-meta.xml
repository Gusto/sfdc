            </connector>
            <label>Check API Integration Escalation</label>
        </rules>
        <rules>
            <name>Is_GEP_Portal_Feed</name>
            <conditionLogic>1 AND 2 AND 3 AND (4 OR 5)</conditionLogic>
            <conditions>
                <leftValueReference>$Record.NetworkScope</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>$Setup.Ardius_Configuration__c.GEP_Chase_Portal_Id__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Visibility</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>AllUsers</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Published</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Type</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>TextPost</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Type</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>AdvancedTextPost</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Coloration_Group_Members</targetReference>
            </connector>
            <label>Is GEP Portal Feed</label>
        </rules>
    </decisions>
    <decisions>
        <name>Check_Ticket_Status</name>
        <label>Check Ticket Status</label>
        <locationX>182</locationX>
        <locationY>539</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Status_is_not_New</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Parent:Ticket__c.Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>New</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Parent:Ticket__c.Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Response Received</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Status_To_Response_Receieved</targetReference>
            </connector>
            <label>Status is not New</label>
        </rules>
    </decisions>
    <decisions>
        <description>Compare Ticket Owner and Feed Item Inserted By. If they are the same, post is created by ticket owner.</description>
        <name>Post_Created_By_Ticket_Owner</name>
        <label>Post Created By Ticket Owner</label>
        <locationX>842</locationX>
        <locationY>539</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Ticket_Owner</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Parent:Ticket__c.OwnerId</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>$Record.CreatedById</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Parent:Ticket__c.Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Awaiting Response</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Status_To_Awaiting_Response</targetReference>
            </connector>
            <label>Ticket Owner</label>
        </rules>
        <rules>
            <name>Non_Ticket_Owner</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Parent:Ticket__c.OwnerId</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>$Record.CreatedById</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Parent:Ticket__c.Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Response Received</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Status_To_Response_Receieved_1</targetReference>
            </connector>
            <label>Non-Ticket Owner</label>
        </rules>
    </decisions>
    <decisions>
        <description>Check if Ticket is Owned by Queue</description>
        <name>Ticket_Owned_By_Queue</name>
