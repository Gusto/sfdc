        <scale>2</scale>
    </formulas>
    <formulas>
        <name>Date_Range_Start_f</name>
        <dataType>String</dataType>
        <expression>TEXT(MONTH({!Queue.Order_Assignment_Start_Date__c})) + &apos;-&apos; + TEXT(DAY({!Queue.Order_Assignment_Start_Date__c})) + &apos;-&apos; + TEXT(YEAR({!Queue.Order_Assignment_Start_Date__c}))</expression>
    </formulas>
    <formulas>
        <name>Team_Plus_Reserve_Available_Capacity_f</name>
        <dataType>Number</dataType>
        <expression>IF({!Get_User_s_Team_Capacity.Available_Capacity__c} &gt; 200,
{!Queue.Reserve_Available_Capacity__c}, {!Queue.Reserve_Available_Capacity__c})</expression>
        <scale>2</scale>
    </formulas>
