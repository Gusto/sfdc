    <description>-BBO-1997: Create engagement case</description>
    <environments>Default</environments>
    <formulas>
        <name>answerIQ</name>
        <dataType>String</dataType>
        <expression>IF( {!Case_Purpose_1} = {!Benefits_Reversal}, &apos;Yes&apos;,&apos;&apos; )</expression>
    </formulas>
    <formulas>
        <name>descriptionFormula</name>
        <dataType>String</dataType>
        <expression>&quot;Hi BenCare, We need a benefits reversal for the employee below – please don’t hesitate to reach out with any questions.&quot;+{!ttLineBreak}+
&quot;EE Deduction Preview:&quot;+{!ttLineBreak}+
&quot;SFDC Link:&quot;+{!ttLineBreak}+
&quot;Benefits that need reversal:&quot;+{!ttLineBreak}+
&quot;Date terminated:&quot;+{!ttLineBreak}+
&quot;Description: &quot;+{!ttLineBreak}+&quot;This EE was terminated on [DATE]. All subscriptions have been re-processed with an end date of [DATE]. Thanks in advance for your help!&quot;</expression>
    </formulas>
    <formulas>
        <name>descriptionValue</name>
        <dataType>String</dataType>
        <expression>IF( {!Case_Purpose_1} = {!Benefits_Reversal}, {!descriptionFormula}, &apos;&apos; )</expression>
    </formulas>
    <formulas>
        <name>reversalSubject</name>
        <dataType>String</dataType>
        <expression>&apos;[Action Required] Benefit Reversal for - &apos; + IF({!Contact.recordId} &lt;&gt; null,{!Contact.recordName}, {!Get_Case_Record.Contact.Name})</expression>
    </formulas>
    <formulas>
        <name>routingCaseReason</name>
        <dataType>String</dataType>
        <expression>IF( {!Case_Purpose_1} = {!Benefits_Reversal}, {!Get_Case_Reason.Id},&apos;&apos; )</expression>
    </formulas>
    <formulas>
        <name>subjectValue</name>
        <dataType>String</dataType>
        <expression>IF( {!Case_Purpose_1} = {!Benefits_Reversal}, {!reversalSubject}, &apos;&apos; )</expression>
    </formulas>
    <formulas>
        <name>supportValue</name>
        <dataType>String</dataType>
        <expression>IF( {!Case_Purpose_1} = {!Benefits_Reversal}, &apos;Tier 2&apos;, &apos;Tier 1&apos; )</expression>
    </formulas>
    <formulas>
        <name>TeamValue</name>
        <dataType>String</dataType>
        <expression>IF( {!Case_Purpose_1} = {!Benefits_Reversal}, &apos;Internal&apos;, &apos;External&apos; )</expression>
    </formulas>
