	/**
	 * @author      Swati Arya
	 * @description	Creates an unique string for usernames
	 */
	public static String buildUniqueString() {
		// Generating a Unique Name
		String strOrgId = UserInfo.getOrganizationId();
		String strDateString = String.valueOf(DateTime.Now()).replace(' ', '').replace(':', '').replace('-', '');
		Integer intRandomVal = Integer.valueOf(Math.rint(Math.random() * 1000000));
		String strUniqueName = strOrgId + strDateString + intRandomVal;

		return ('usr' + strUniqueName + '@test' + strOrgId + '.org');
	}

