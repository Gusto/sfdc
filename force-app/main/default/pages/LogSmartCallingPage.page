	<script>
		function fillComment(val) {
			if ('{!currentObject}' != 'Benefit Order' &&
				(document.getElementById('pg:frm:phoneNumber').value == null || document.getElementById('pg:frm:phoneNumber').value == '') &&
				(document.getElementById('pg:frm:hdWhoId').value == null || document.getElementById('pg:frm:hdWhoId').value == '')) {
				alert('Attempting to log a call on wrong Contact!!!');
			} else {
				avlVal = document.getElementById('pg:frm:mainpb:pb6:txtArea').value;
				var recordId = document.getElementById('pg:frm:recordId').value;
				if (val == '') { return; }
				if (avlVal.startsWith('Connect') || avlVal.startsWith('Voicemail') || avlVal.startsWith('Attempt')) {
					avlVal = avlVal.replace(/^\S+/g, val);
					document.getElementById('pg:frm:mainpb:pb6:txtArea').value = '';
					document.getElementById('pg:frm:mainpb:pb6:txtArea').value = avlVal;
				} else {
					var comment = val + ' || ' + '{!DateTimeStr}' + ' || ' + '{!$User.FirstName}' + ' ' + '{!$User.LastName}';
					comment = comment + '\n' + document.getElementById('pg:frm:mainpb:pb6:txtArea').value;
					document.getElementById('pg:frm:mainpb:pb6:txtArea').value = comment;
				}
				if ('{!currentObject}' == 'Benefit Order') {
					var notes = document.getElementById('pg:frm:mainpb:pb6:txtArea').value;
					if (notes != undefined && notes.search('Notes:') == -1) {
						document.getElementById('pg:frm:mainpb:pb6:txtArea').value = notes + '\nNotes: \n' + 'Action Items: \n';
					}
					return true;
				}
				avlSubject = document.getElementById('pg:frm:mainpb:pb6:txtSubject').value;
				if (avlSubject.startsWith('Connect') || avlSubject.startsWith('Voicemail') || avlSubject.startsWith('Attempt')) {
					avlSubject = avlSubject.replace(/^\S+/g, val);
					document.getElementById('pg:frm:mainpb:pb6:txtSubject').value = '';
					document.getElementById('pg:frm:mainpb:pb6:txtSubject').value = avlSubject;
				} else {
					var comment = val + ' || ' + '{!DateTimeStr}' + ' || ' + '{!$User.FirstName}' + ' ' + '{!$User.LastName}';
					comment = comment + '\n' + document.getElementById('pg:frm:mainpb:pb6:txtSubject').value;
					document.getElementById('pg:frm:mainpb:pb6:txtSubject').value = comment;
				}
			}
			return true;
		}
		function removePrimaryTabParams() {
			removeClass();
			if ('{!currentObject}' == 'Benefit Order') {
				sforce.console.getEnclosingPrimaryTabId(function (result) {
					var objBOId = '{!objBenefitOrder.Id}';
					var redirectUrl = '/' + objBOId + '?o=1';
					sforce.console.getTabLink(sforce.console.TabLink.PARENT_AND_CHILDREN, result.id, function (tabLinkRes) {
						var str = decodeURIComponent(tabLinkRes.tabLink).split('#')[1];
						console.log(str);
						var allURLs = str.split('|');

						sforce.console.openPrimaryTab(result.id, redirectUrl, true, '{!objBenefitOrder.Name}', function (resultNew) {
							console.log(allURLs);
							for (var indx = 0; indx < allURLs.length; indx++) {
								if (redirectUrl != allURLs[indx] && allURLs[indx] != '')
									sforce.console.openSubtab(resultNew.id, allURLs[indx], false);
							}
						});
					});

				});
			}
		}

		function validateForm() {
			document.getElementById('disErr').innerHTML = '';

			if (document.getElementById('pg:frm:mainpb:pb3:dispotion').value == '') {
				document.getElementById('disErr').innerHTML = 'Please select Disposition.';
				document.getElementById('pg:frm:mainpb:pb3:dispotion').focus();
			} else if (('{!currentUserRole}' == 'sales_sb_ob_xx' || '{!currentUserRole}' == 'sales_sb_ob_sme') && document.getElementById('pg:frm:mainpb:pb3:dispotion').value == 'Connect' && document.getElementById('pg:frm:mainpb:pb3:objectives').value == '') {
				document.getElementById('disErrObj').innerHTML = 'Please select a Call Objective.';
				document.getElementById('pg:frm:mainpb:pb3:objectives').focus();
			} else if ('{!currentObject}' != 'Benefit Order' &&
				(document.getElementById('pg:frm:phoneNumber').value == null || document.getElementById('pg:frm:phoneNumber').value == '') &&
				document.getElementById('pg:frm:hdWhoId').value == '') {
				document.getElementById('disErrWhoId').innerHTML = 'Attempting to log a call on wrong Contact!!!';

			} else if ('{!currentObject}' == 'Benefit Order') {

				if (document.getElementById('pg:frm:mainpb:pb3:callType').value == null || document.getElementById('pg:frm:mainpb:pb3:callType').value == '') {
					document.getElementById('disErrCallType').innerHTML = 'Please select Call Type.';
				}
				else if (document.getElementById('pg:frm:mainpb:pb3:reasonforCall').value == null || document.getElementById('pg:frm:mainpb:pb3:reasonforCall').value == '') {
					document.getElementById('disErrReason').innerHTML = 'Please select Reason for Call.';
				}
				else if (document.getElementById('pg:frm:mainpb:pb3:calendly').value == null
					|| document.getElementById('pg:frm:mainpb:pb3:calendly').value == ''
					|| document.getElementById('pg:frm:mainpb:pb3:calendly').value == 'None') {
					document.getElementById('disErrCalendly').innerHTML = 'Please select Calendly.';
				}
				else {
					saveTask();
				}
			} else {
				saveTask();
			}
			return false;
		}

