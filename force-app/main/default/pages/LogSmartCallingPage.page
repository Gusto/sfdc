			else if (('{!currentUserRole}' == 'sales_sb_ob_xx' || '{!currentUserRole}' == 'sales_sb_ob_sme') && document.getElementById('pg:frm:mainpb:pb3:dispotion').value == 'Connect' && document.getElementById('pg:frm:mainpb:pb3:objectives').value == '') {
