		function checkDate() {
			var idate = document.getElementById("datepicker"),
				resultDiv = document.getElementById("datewarn"),
				dateReg = /(0[1-9]|[12][0-9]|3[01])[\/](0[1-9]|1[012])[\/]201[4-9]|20[2-9][0-9]/;

			if (!dateReg.test(idate.value)) {
				resultDiv.innerHTML = "Invalid date!";
				resultDiv.style.color = "red";
				return;
			}

			if (isFutureDate(idate.value)) {
				resultDiv.innerHTML = "Entered date is a future date";
				resultDiv.style.color = "red";
			} else {
				resultDiv.innerHTML = "It's a valid date";
				resultDiv.style.color = "green";
			}
		}

		function startPlay(id) {
			recordId = id;
			if (id == '') {
				alert('There are no records to play');
			} else {
				sforce.console.setCustomConsoleComponentVisible(false);
				if (sforce.console.isInConsole())
					sforce.console.getEnclosingTabId(openSubtab);
				else
					window.top.location.href = '/' + id;
			}
		}

		var openSubtab = function openSubtab(result) {
			//Now that we have the primary tab ID, we can open a new subtab in it
			var primaryTabId = result.id;
			sforce.console.openPrimaryTab(null, '/' + recordId + '?isplay=1', true);
			sforce.console.closeTab(result.id);
		};

		function openRec(recordid) {
			sforce.console.openPrimaryTab(null, "/" + recordid, true);
		}
		var getTabLink = function getTabLink(result) {
			sforce.console.getTabLink(sforce.console.TabLink.TAB_ONLY,
				result.id, showTabLink);
		}
		var showTabLink = function showTabLink(result) {
			var link = result.tabLink;
			var decoded = decodeURIComponent(link);
			var phoneNumberlink = getUrlParameter('ani', decoded);
			// For inbound calls, we get the phone and genid parameters from CreateSalesCallTrackingPage redirection on Benefit Order page
			if (phoneNumberlink == undefined) {
				phoneNumberlink = getUrlParameter('phone', decoded);
			}

			var genesysidlink = getUrlParameter('_c', decoded);
			if (genesysidlink == undefined) {
				genesysidlink = getUrlParameter('genid', decoded);
			}

			var recordIdLink = getUrlParameter('id', decoded);
			document.getElementById('pg:frm:phoneNumber').value = phoneNumberlink;
			document.getElementById('pg:frm:integrationId').value = genesysidlink;
			document.getElementById('pg:frm:recordId').value = recordIdLink;

			if (phoneNumberlink != '' && phoneNumberlink != null && phoneNumberlink != undefined && '{!currentObject}' == 'Benefit Order') {
				document.getElementById('pg:frm:mainpb:pb3:dispotion').value = 'Connect';
				document.getElementById('pg:frm:mainpb:pb3:callType').value = 'Inbound';
				fillComment('Connect');
			}
		};

		var getUrlParameter = function getUrlParameter(sParam, decodedLink) {
			var sPageURL = decodedLink,
				sURLVariables = sPageURL.split('&'),
				sParameterName,
				i;

			for (i = 0; i < sURLVariables.length; i++) {
				sParameterName = sURLVariables[i].split('=');

				if (sParameterName[0] == sParam) {
					return sParameterName[1];
				}
			}
		};
		window.onload = function () {
			document.getElementById("pg:frm:mainpb:hiOppSection").style.display = 'none';
			document.getElementById("pg:frm:mainpb:payrollOppSection").style.display = 'none';
			document.getElementById(" pg:frm:mainpb:j_id113:j_id114:4:j_id115").className = 'form-control';

		}
		document.onready = function () {
			sforce.console.getFocusedPrimaryTabId(getTabLink);
		}
	</script>

