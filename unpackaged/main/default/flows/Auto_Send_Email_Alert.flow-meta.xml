    <description>- Added Case TAM routing
- Updated Auto Close Date Time for Engagement RT.
- Updated Case Status for auto-solve cases to be Awaiting Response for Engagement RT.
- Send an email using text from Case&apos;s Auto Response Email Content field.</description>
    <environments>Default</environments>
    <formulas>
        <name>AutoCloseDateTime</name>
        <dataType>DateTime</dataType>
        <expression>IF({!CaseStatus} == &apos;Awaiting Response&apos; &amp;&amp; {!Case.RecordType.DeveloperName} == &apos;Engagement&apos;, {!Case.Auto_Close_Date_Time__c}, null)</expression>
    </formulas>
    <formulas>
        <description>If Routing Case Reason Classification.Do_Not_Auto_Solve_for_Auto_Email__c == false, status will be Solved. Else, don&apos;t change status.</description>
        <name>CaseStatus</name>
        <dataType>String</dataType>
        <expression>IF ({!Case.Routing_Case_Reason_Classification__r.Do_Not_Auto_Solve_for_Auto_Email__c}, &apos;Awaiting Response&apos;, 
IF (TEXT({!Case.Routing_Case_Reason_Classification__r.Type__c}) == &apos;Engagement&apos;, &apos;Awaiting Response&apos;, &apos;Solved&apos;))</expression>
    </formulas>
    <formulas>
        <description>If RT is Engagement queue Id will be Engagement Case, otherwise Engagement-Auto First Response Queue</description>
        <name>OwnerQueueId</name>
        <dataType>String</dataType>
        <expression>IF(
{!Case.RecordType.Name} = &apos;Engagement&apos;, 
{!$Setup.Engagement_Flow_Configuration__c.Engagement_Cases_Queue_ID__c},
{!$Setup.Engagement_Flow_Configuration__c.Engagement_Auto_First_Response_Queue_Id__c}
)</expression>
    </formulas>
