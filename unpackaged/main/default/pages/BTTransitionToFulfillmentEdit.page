		function openmultieinttf(strTTFIds, blnIsError, blnShowOrderTypePage) {
			console.log("passed str 1 " + strTTFIds);
			if (strTTFIds != null && strTTFIds != undefined && strTTFIds != "") {
				var ttfIds = strTTFIds.split(";");
				console.log("ttfID " + ttfIds);
				var i;
				for (i = 0; i < ttfIds.length; i++) {
					if (sforce.console.isInConsole()) {
						console.log("single ttf " + ttfIds[i]);
						sforce.console.openPrimaryTab(null, "/" + ttfIds[i] + "/e?isFT=1&retURL=" + ttfIds[i], false);
					} else {
						window.open("/" + ttfIds[i] + "/e?isFT=1&retURL=" + ttfIds[i]);
					}
				}
			}

			if (String(blnIsError) == "false" && String(blnShowOrderTypePage) == "false") {
				if (sforce.console.isInConsole()) {
					closeTab();
				} else {
					var oppId = "{!objTTFRecord.Opportunity__c}";
					window.open("/" + oppId, "_parent", "");
				}
			}
		}

