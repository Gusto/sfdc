<apex:page standardController="Transition_to_Fulfillment__c" extensions="BTTransitionToFulfilmentExtension" id="mainpage" title="Benefits Transfer: Transition To Fulfillment" docType="HTML-5.0">
	<apex:slds />
	<c:BTCustomCSS />
	<apex:includeScript value="/support/console/45.0/integration.js" />
	<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.2/css/all.css" integrity="sha384-fnmOCqbTlWIlj8LyTjo7mOUStjsKC4pOpQbqyi7RrhN7udi9RwhKkMHpvLbHG9Sr" crossorigin="anonymous" />
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
	<apex:form id="mainform">
		<div class="slds-scope">
			<c:GustoActionStatus />
			<apex:sectionHeader title="Transition To Fulfillment" subtitle="Details" />
			<apex:pageMessages id="messages" />
			<apex:outputPanel layout="none" rendered="{!!blnShowOrderTypePage}">
				<apex:pageBlock id="pageblock">
					<apex:pageBlockSection columns="2" title="TTF Detail">
						<apex:inputField value="{!objTTFRecord.Opportunity__c}" />
						<apex:inputField value="{!objTTFRecord.Account__c}" />
						<apex:outputField value="{!objTTFRecord.Benefit_Order__c}" />
						<apex:inputField value="{!objTTFRecord.BYB_to_BoR__c}" rendered="{!!blnIsBYOBOpportunity}" />
						<apex:inputField value="{!objTTFRecord.Handed_off_to_preferred_broker_partner__c}" rendered="{!blnIsBYOBOpportunity}" >
							<apex:actionSupport event="onchange" action="{!updatePreferedBroker}" rerender="pageblock" status="actStatusId"/>
						</apex:inputField>
						<apex:pageBlockSectionItem rendered="{!blnIsBYOBOpportunity}" >
							<apex:outputLabel value="SGHI/LGHI" />
							<apex:outputPanel layout="block" styleclass="requiredInput">
								<div class="requiredBlock" style="display:{!if(blnIsBYOBOpportunity,'','none;')}"/>
								<apex:inputField value="{!objRelatedOpp.SGHI_LGHI__c}" />
							</apex:outputPanel>
						</apex:pageBlockSectionItem>
						<apex:pageBlockSectionItem rendered="{!blnIsBYOBOpportunity}" >
							<apex:outputLabel value="Preferred Broker" style="display:{!if( (blnIsBYOBOpportunity && objTTFRecord.Handed_off_to_preferred_broker_partner__c),'','none;')}"/>
							<apex:outputPanel layout="block" styleclass="requiredInput" style="display:{!if( (blnIsBYOBOpportunity && objTTFRecord.Handed_off_to_preferred_broker_partner__c),'','none;')}">
								<div class="requiredBlock"/>
								<apex:inputField value="{!objTTFRecord.Preferred_Broker__c}" />
							</apex:outputPanel>
						</apex:pageBlockSectionItem>
						<apex:pageBlockSectionItem rendered="{!blnIsBYOBOpportunity}" >
							<apex:outputLabel value="Funding Type" />
							<apex:outputPanel layout="block" styleclass="requiredInput">
								<div class="requiredBlock" style="display:{!if(blnIsBYOBOpportunity,'','none;')}"/>
								<apex:inputField value="{!objRelatedOpp.Funding_Type__c}" />
							</apex:outputPanel>
						</apex:pageBlockSectionItem>
						<apex:pageBlockSectionItem rendered="{!blnIsBYOBOpportunity}" >
							<apex:outputLabel value="PEO Switcher" />
							<apex:outputPanel layout="block" styleclass="requiredInput">
								<div class="requiredBlock" style="display:{!if(blnIsBYOBOpportunity,'','none;')}"/>
								<apex:inputField value="{!objTTFRecord.PEO__c}" />
							</apex:outputPanel>
						</apex:pageBlockSectionItem>
						<apex:inputField value="{!objRelatedOpp.Unsupported_Carrier__c}" rendered="{!blnIsBYOBOpportunity}" />
						<apex:pageBlockSectionItem rendered="{!blnIsBYOBOpportunity}">
							<apex:outputLabel value="Broker Loyal"/>
							<apex:outputPanel layout="block" styleclass="requiredInput">
								<div class="requiredBlock"/>
								<apex:inputField value="{!objTTFRecord.Broker_Loyal__c }" />
							</apex:outputPanel>
						</apex:pageBlockSectionItem>
						<apex:inputField value="{!objTTFRecord.Compliance_Upsells__c }" rendered="{!blnIsBYOBOpportunity}" />
                        <apex:pageBlockSectionItem rendered="{!blnIsBYOBOpportunity}" />
						<apex:pageBlockSectionItem rendered="{!blnIsBYOBOpportunity}">
							<apex:outputLabel value="BoR Eligible Medical"/>
							<apex:outputPanel layout="block" styleclass="requiredInput">
								<div class="requiredBlock"/>
								<apex:inputField value="{!objTTFRecord.BoR_Eligible_Medical__c }" >
									<apex:actionSupport event="onchange" action="{!updateBoREligibleMedical}" rerender="pageblock" status="actStatusId"/>
								</apex:inputField>
							</apex:outputPanel>
						</apex:pageBlockSectionItem>
					</apex:pageBlockSection>   
					<apex:pageBlockSection columns="1">
						<apex:pageBlockSectionItem >
							<apex:outputLabel value="BYB Reason" style="display:{!if((blnIsBYOBOpportunity && blnBYBReason),'','none;')}"/>
							<apex:outputPanel layout="block" styleclass="requiredInput" style="display:{!if((blnIsBYOBOpportunity && blnBYBReason),'','none;')}">
								<div class="requiredBlock"/>
								<apex:inputField value="{!objTTFRecord.BYB_Reason__c}" />
							</apex:outputPanel>
						</apex:pageBlockSectionItem>        
					</apex:pageBlockSection>   
					<apex:pageBlockSection columns="2">
						<apex:outputField value="{!objTTFRecord.Opportunity__r.Partner_Tier__c}" rendered="{! if(objTTFRecord.Id != null && objTTFRecord.Opportunity__r.Partner_Tier__c != null,true,false)}"
						/>
						<apex:outputField value="{!objTTFRecord.Opportunity__r.Partner_Firm_Name__c}"
							rendered="{! if(objTTFRecord.Id != null && objTTFRecord.Opportunity__r.Partner_Firm_Name__c != null,true,false)}"
						/>
						<apex:pageBlockSectionItem rendered="{! if(objTTFRecord.Id == null && strPartnerTier != null,true,false)}">
							<apex:outputLabel value="Partner Tier"></apex:outputLabel>
							<apex:outputText value="{!strPartnerTier}" />
						</apex:pageBlockSectionItem>
						<apex:pageBlockSectionItem rendered="{! if(objTTFRecord.Id == null && strPartnerFirmName != null,true,false)}">
							<apex:outputLabel value="Partner Firm Name"></apex:outputLabel>
							<apex:outputText value="{!strPartnerFirmName}" />
						</apex:pageBlockSectionItem>
					</apex:pageBlockSection>
					<apex:pageBlockSection columns="1" id="additionalPOCSection" title="Opportunity Contact Roles" collapsible="true">
						<apex:outputPanel rendered="{!AND(NULL != list_OpptyContactRole , list_OpptyContactRole.size > 0)}" id="contactRoleSection">
							<table class="slds-table slds-table_cell-buffer slds-table_bordered">
								<thead>
									<tr class="slds-text-title slds-line-height_reset">
										<th scope="col">Contact Name</th>
										<th scope="col">Role</th>
										<th scope="col">Check Primary POC</th>
										<th scope="col">Check Additional POC</th>
										<th scope="col" style="display:{!if(blnIsBYOBOpportunity,'','none;')}">Check Broker POC</th>
									</tr>
								</thead>
								<tbody>
									<apex:repeat value="{!list_POCWrapper}" var="cRole">
										<tr class="slds-text-title detailRow">
											<td>
												<apex:outputText value="{!cRole.objContactRole.Contact.FirstName} {!cRole.objContactRole.Contact.LastName}" />
											</td>
											<td>
												<apex:repeat value="{!map_ContactIdRole}" var="contactId" >
													<apex:outputPanel rendered="{!IF(contactId == cRole.objContactRole.ContactId,true,false)}">
														<apex:outputText value="{!map_ContactIdRole[contactId]}" />
													</apex:outputPanel>
												</apex:repeat>
											</td>
											<td>
												<apex:inputCheckbox value="{!cRole.boolChkPrimary}">
													<apex:actionSupport event="onchange" action="{!selectPrimary}" rerender="additionalPOCSection,additionalPOCSectionDetail" status="actStatusId">
														<apex:param value="{!cRole.objContactRole.Id}" name="roleId" assignTo="{!idPrimaryRole}" />
													</apex:actionSupport>
												</apex:inputCheckbox>
											</td>
											<td>
												<apex:inputField value="{!cRole.objContactRole.Check_Additional_POC__c}">
													<apex:actionSupport event="onchange" action="{!selectSecondary}" rerender="additionalPOCSection,additionalPOCSectionDetail" status="actStatusId">
														<apex:param value="{!cRole.objContactRole.Id}" name="roleId" assignTo="{!idSecRole}" />
													</apex:actionSupport>
												</apex:inputField>
											</td>
											<td style="display:{!if(blnIsBYOBOpportunity,'','none;')}">
												<apex:inputField value="{!cRole.objContactRole.Check_Broker_POC__c}">
													<apex:actionSupport event="onchange" action="{!selectBroker}" rerender="additionalPOCSection,additionalPOCSectionDetail" status="actStatusId">
														<apex:param value="{!cRole.objContactRole.Id}" name="roleId" assignTo="{!idBrokerRole}" />
													</apex:actionSupport>
												</apex:inputField>
											</td>
										</tr>
									</apex:repeat>
								</tbody>
							</table>
						</apex:outputPanel>
						<apex:outputPanel rendered="{!OR(NULL == list_OpptyContactRole, list_OpptyContactRole.size == 0)}"> There are no Contact Roles on opportunity. </apex:outputPanel>
					</apex:pageBlockSection>
					<apex:pageBlockSection columns="2" id="additionalPOCSectionDetail">
						<apex:pageBlockSectionItem >
							<apex:outputLabel value="Primary Point of Contact" />
							<apex:outputPanel layout="block" styleclass="requiredInput">
								<div class="requiredBlock" />
								<apex:inputField value="{!objTTFRecord.Contact__c}" />
							</apex:outputPanel>
						</apex:pageBlockSectionItem>
						<apex:pageBlockSectionItem rendered="{!!blnIsBYOBOpportunity}" />
						<apex:pageBlockSectionItem rendered="{!blnIsBYOBOpportunity}">
							<apex:outputLabel value="Broker Point of Contact" />
							<apex:outputPanel layout="block" styleclass="requiredInput">
								<div class="requiredBlock" />
								<apex:inputField value="{!objTTFRecord.Broker_Point_of_Contact__c}" />
							</apex:outputPanel>
						</apex:pageBlockSectionItem>
						<apex:inputField value="{!objTTFRecord.Point_of_Contact_Phone__c}" />
						<apex:outputLabel />
						<apex:inputField value="{!objTTFRecord.Point_of_Contact_Email__c}" />
					</apex:pageBlockSection>
					<apex:pageBlockSection columns="1" title="Carrier Names & Renewal Date">
						<apex:outputPanel id="CarrierNames">
							<apex:outputPanel style="display:{!if(blnIsErrorOtherCarrier,'','none')};" styleClass="slds-select">
								<apex:pageMessage severity="error" strength="1" summary="{!strErrorMessage}" />
							</apex:outputPanel>
							<apex:outputPanel rendered="{!blnIsBYOBOpportunity && objTTFRecord.Opportunity__r.Order_Generated__c == false}">
								<apex:commandButton value="Add Carrier" action="{!addNewCarrierRow}" style="float: right; margin-bottom: 7px" reRender="CarrierNames" />
							</apex:outputPanel>

							<table class="slds-table slds-table_cell-buffer slds-table_bordered">
								<thead>
									<tr class="slds-text-title slds-line-height_reset">
										<th scope="col">Carrier Name</th>
										<th scope="col">Renewal Date</th>
										<th scope="col">Benefits</th>
										<th scope="col" style="display:{!if(blnIsBYOBOpportunity && objTTFRecord.Opportunity__r.Order_Generated__c == false,'','none;')}"></th>
									</tr>
								</thead>
								<tbody>
									<apex:repeat value="{!map_carrierKeyBenefitDetails}" var="benefitWrapper" rendered="{!!blnIsBYOBOpportunity}">
										<tr class="slds-text-title detailRow">
											<td>
												<a class="slds-form-element__label" href="/{!map_carrierKeyBenefitDetails[benefitWrapper].objCarrier.Id}"
													>{!map_carrierKeyBenefitDetails[benefitWrapper].objCarrier.Name}</a
												>
											</td>
											<td>
												<apex:outputPanel layout="block" styleclass="requiredInput" rendered="{! if(objTTFRecord.Opportunity__r.Order_Generated__c == true,false,true)}">
													<div class="requiredBlock" />
													<apex:inputField value="{!map_carrierKeyBenefitDetails[benefitWrapper].objContact.Last_Activity_Date__c}" label="" />
												</apex:outputPanel>
												<apex:outputField value="{!map_carrierKeyBenefitDetails[benefitWrapper].objContact.Last_Activity_Date__c}"
													label=""
													rendered="{! if(objTTFRecord.Opportunity__r.Order_Generated__c == true,true,false)}"
												/>
											</td>
											<td>
												<div>
													<apex:variable var="benefitsName" value="{!''}{!map_carrierKeyBenefitDetails[benefitWrapper].lstBenefitName}" />
													<span class="slds-badge slds-badge_inverse custom-badge" style="display:{!IF(CONTAINS(benefitsName, 'medical'), '','none')}">Medical</span>
													<span class="slds-badge slds-badge_inverse custom-badge" style="display:{!IF(CONTAINS(benefitsName, 'dental'), '','none')}">Dental</span>
													<span class="slds-badge slds-badge_inverse custom-badge" style="display:{!IF(CONTAINS(benefitsName, 'vision'), '','none')}">Vision</span>
													<span class="slds-badge slds-badge_inverse custom-badge" style="display:{!IF(CONTAINS(benefitsName, 'life'), '','none')}">Life</span>
													<span class="slds-badge slds-badge_inverse custom-badge" style="display:{!IF(CONTAINS(benefitsName, 'long_term_disability'), '','none')}">LTD</span>
													<span class="slds-badge slds-badge_inverse custom-badge" style="display:{!IF(CONTAINS(benefitsName, 'short_term_disability'), '','none')}">STD</span>
												</div>
											</td>
											<td style="display:{!if(blnIsBYOBOpportunity && objTTFRecord.Opportunity__r.Order_Generated__c == false,'','none;')}" />
										</tr>
									</apex:repeat>
									<apex:repeat value="{!list_CarrierDetails}" var="CarrierDetail" rendered="{!blnIsBYOBOpportunity}">
										<tr class="slds-text-title detailRow">
											<td>
												<a class="slds-form-element__label" href="/{!CarrierDetail.objCase.Carrier__c}" style="display:{!if(CarrierDetail.blnIsSaved,'','none;')}"
													>{!CarrierDetail.strCarrierName}</a
												>
												<apex:inputField value="{!CarrierDetail.objCase.Carrier__c}"
													style="display:{!if(CarrierDetail.blnIsSaved,'none;','')}"
													onchange="showOtherDetail(this.value, {!CarrierDetail.intRecordIndex})"
												/>
												<br />
												<apex:input type="text"
													styleClass="OtherCarrierName{!CarrierDetail.intRecordIndex}"
													value="{!CarrierDetail.strOtherCarrier}"
													style="display:{!if(!CarrierDetail.blnIsSaved && (CarrierDetail.strOtherCarrier != '' || CarrierDetail.blnIsOtherTextBoxDisplayed),'','none;')};"
												/>
												<apex:outputLabel styleClass="OtherCarrierName{!CarrierDetail.intRecordIndex}"
													value="{!CarrierDetail.strOtherCarrier}"
													style="display:{!if(CarrierDetail.blnIsSaved && CarrierDetail.strOtherCarrier != '','','none;')};"
												/>
											</td>
											<td>
												<apex:inputField value="{!CarrierDetail.objContact.Last_Activity_Date__c}" style="display:{!if(CarrierDetail.blnIsSaved,'none;','')}" />
												<apex:outputField value="{!CarrierDetail.objContact.Last_Activity_Date__c}" label="" rendered="{!CarrierDetail.blnIsSaved}" />
											</td>
											<td>
												<apex:selectCheckboxes value="{!CarrierDetail.list_SelectedBenefitNames}" layout="lineDirection" style="display:{!if(CarrierDetail.blnIsSaved,'none;','')}">
													<apex:selectOptions value="{!list_BenefitTypes}" />
												</apex:selectCheckboxes>
												<div style="display:{!if(CarrierDetail.blnIsSaved,'','none;')}">
													<apex:repeat value="{!CarrierDetail.list_SelectedBenefitNames}" var="BenefitTypes">
														<span class="slds-badge slds-badge_inverse custom-badge">{!BenefitTypes}</span>
													</apex:repeat>
												</div>
											</td>
											<td style="display:{!if(blnIsBYOBOpportunity && objTTFRecord.Opportunity__r.Order_Generated__c == false,'','none')}; text-align:center">
												<span class="info-tooltip" style="position: relative;padding: 2px;display:{!if(CarrierDetail.blnIsSaved,'none;','')}">
													<apex:commandLink style="position: relative" action="{!saveCarriers}" reRender="CarrierNames" status="actStatusId">
														<apex:param name="Saveindex" value="{!CarrierDetail.intRecordIndex}" assignTo="{!intSaveIndx}" />
														<i class="fas fa-save" style="font-size: 20px"></i>
													</apex:commandLink>
												</span>
												<span class="info-tooltip" style="position: relative;padding: 2px;display:{!if(CarrierDetail.blnIsSaved,'','none;')}">
													<apex:commandLink style="position: relative" action="{!editCarrierName}" reRender="CarrierNames" status="actStatusId">
														<apex:param name="Editindex" value="{!CarrierDetail.intRecordIndex}" assignTo="{!intEditIndx}" />
														<i class="fas fa-edit" style="font-size: 20px"></i>
													</apex:commandLink>
												</span>
												<span class="info-tooltip" style="position: relative">
													<apex:commandLink style="position: relative; padding: 2px" action="{!deleteCarrierName}" reRender="CarrierNames" status="actStatusId">
														<apex:param name="Deleteindex" value="{!CarrierDetail.intRecordIndex}" assignTo="{!intDeleteIndx}" />
														<i class="fas fa-trash" style="font-size: 20px"></i>
													</apex:commandLink>
												</span>
											</td>
										</tr>
									</apex:repeat>
									<tr>
										<td style="width: 10%">
											<label class="slds-form-element__label">Notes for the CX team</label>
										</td>
										<td colspan="3" style="width: 80%">
											<apex:inputField value="{!objTTFRecord.Notes_for_the_CX_team__c}" style="width: 100%" />
										</td>
									</tr>
								</tbody>
							</table>
						</apex:outputPanel>
					</apex:pageBlockSection>
					<apex:pageBlockSection columns="2" id="detailsection">
						<apex:pageBlockSection columns="1" id="tadainfo" title="TaDa Information" collapsible="true" rendered="{!!blnIsBYOBOpportunity}">
							<div class="slds-form-element" style="display: flex">
								<label class="slds-form-element__label label-yesno" for="">Does order contain TaDa?</label>
								<div class="slds-form-element__control slds-input-has-icon slds-input-has-icon_right yes-no-picklist">
									<select class="slds-select tada-select">
										<option>Select</option>
										<option>Yes</option>
										<option>No</option>
									</select>
								</div>
							</div>
							<apex:outputPanel styleClass="tada-select-panel" style="display: none">
								<table style="margin-top: 2%">
									<thead>
										<th>Benefits</th>
										<th>Is Selected</th>
										<th style="text-align: center">Renewal Date</th>
									</thead>
									<tbody>
										<tr>
											<td class="tada-benefit-column">
												<label class="slds-form-element__label tada-label" for="">Is there an HSA?</label>
											</td>
											<td>
												<div class="slds-form-element__control slds-input-has-icon slds-input-has-icon_right">
													<apex:selectList styleclass="slds-select tada-select-hsa"
														size="1"
														value="{!optedTadaDetails.strIsHSASelected}"
														multiselect="false"
														rendered="{! if(objTTFRecord.Opportunity__r.Order_Generated__c == true,false,true)}"
													>
														<apex:selectOption itemValue="" itemLabel="-None-" />
														<apex:selectOption itemValue="Transfer" itemLabel="Transfer" />
														<apex:selectOption itemValue="New" itemLabel="New" />
													</apex:selectList>
													<apex:outputText styleclass="tada-select-hsa"
														value="{!optedTadaDetails.strIsHSASelected}"
														rendered="{! if(objTTFRecord.Opportunity__r.Order_Generated__c == true,true,false)}"
													/>
												</div>
											</td>
											<td style="text-align: center" class="tada-benefit-column">
												<div style="display: none" class="tada-renewaldate-hsa required-Div">
													<apex:outputPanel layout="block" styleclass="requiredInput" rendered="{! if(objTTFRecord.Opportunity__r.Order_Generated__c == true,false,true)}">
														<div class="requiredBlock" />
														<apex:inputField value="{!objTTFRecord.HSA_Renewal_Date__c}" label="" />
													</apex:outputPanel>
													<apex:outputField value="{!objTTFRecord.HSA_Renewal_Date__c}"
														label=""
														rendered="{! if(objTTFRecord.Opportunity__r.Order_Generated__c == true,true,false)}"
													/>
												</div>
											</td>
										</tr>
										<tr>
											<td class="tada-benefit-column">
												<label class="slds-form-element__label tada-label" for="">Is there an FSA?</label>
											</td>
											<td>
												<div class="slds-form-element__control slds-input-has-icon slds-input-has-icon_right">
													<apex:selectList styleclass="slds-select tada-select-fsa"
														size="1"
														value="{!optedTadaDetails.strIsFSASelected}"
														multiselect="false"
														rendered="{! if(objTTFRecord.Opportunity__r.Order_Generated__c == true,false,true)}"
													>
														<apex:selectOption itemValue="" itemLabel="-None-" />
														<apex:selectOption itemValue="Transfer" itemLabel="Transfer" />
														<apex:selectOption itemValue="New" itemLabel="New" />
													</apex:selectList>
													<apex:outputText styleclass="tada-select-fsa"
														value="{!optedTadaDetails.strIsFSASelected}"
														rendered="{! if(objTTFRecord.Opportunity__r.Order_Generated__c == true,true,false)}"
													/>
												</div>
											</td>
											<td style="text-align: center" class="tada-benefit-column">
												<div style="display: none" class="tada-renewaldate-fsa required-Div">
													<apex:outputPanel layout="block" styleclass="requiredInput" rendered="{! if(objTTFRecord.Opportunity__r.Order_Generated__c == true,false,true)}">
														<div class="requiredBlock" />
														<apex:inputField value="{!objTTFRecord.FSA_Renewal_Date__c}" label="" />
													</apex:outputPanel>
													<apex:outputField value="{!objTTFRecord.FSA_Renewal_Date__c}"
														label=""
														rendered="{! if(objTTFRecord.Opportunity__r.Order_Generated__c == true,true,false)}"
													/>
												</div>
											</td>
										</tr>
										<tr>
											<td class="tada-benefit-column">
												<label class="slds-form-element__label tada-label" for="">Is there an DCA?</label>
											</td>
											<td>
