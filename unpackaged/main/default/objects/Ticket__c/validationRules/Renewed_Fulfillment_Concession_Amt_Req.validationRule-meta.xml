<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Renewed_Fulfillment_Concession_Amt_Req</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
   RecordType.Name = &quot;Renewed Fulfillment&quot;,
   Concessions__c = TRUE,
   ISBLANK(Concession_Amount__c)
)</errorConditionFormula>
    <errorDisplayField>Concession_Amount__c</errorDisplayField>
    <errorMessage>Concession Amount is Required</errorMessage>
</ValidationRule>
