<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Employee_ID_Must_Be_Populated</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
ISBLANK( EmployeeNumber )
)</errorConditionFormula>
    <errorMessage>Employee Number is required.</errorMessage>
</ValidationRule>
