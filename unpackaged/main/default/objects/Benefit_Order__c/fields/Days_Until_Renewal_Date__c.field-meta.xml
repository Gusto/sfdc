<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Days_Until_Renewal_Date__c</fullName>
    <description>This gives the days until the renewal date</description>
    <externalId>false</externalId>
    <formula>IF(
!Closed__c,
Renewal_Date__c - TODAY(),
VALUE(&apos;&apos;)
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>This gives the days until the renewal date</inlineHelpText>
    <label>Days Until Renewal Date</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
