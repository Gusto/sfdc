<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Negative_discount</fullName>
    <active>true</active>
    <description>Discount cannot be negative.</description>
    <errorConditionFormula>Discount__c &lt; 0</errorConditionFormula>
    <errorDisplayField>Discount__c</errorDisplayField>
    <errorMessage>Discount cannot be negative.</errorMessage>
</ValidationRule>
