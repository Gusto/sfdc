<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Active_Inactive__c</fullName>
    <externalId>false</externalId>
    <formula>IF(DATEVALUE(CreatedDate) &gt;= TODAY()-30, &apos;Active&apos;, &apos;Inactive&apos;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Active = Created in the last 30 Days
Inactive = Created more that 30 Days ago</inlineHelpText>
    <label>Active/Inactive</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
