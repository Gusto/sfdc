<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Group_Has_Hawaii_EE__c</fullName>
    <description>Marked as true if any EE under the account resides in Hawaii</description>
    <formula>IF(INCLUDES(Account.Resident_States__c,&quot;HI&quot;),TRUE,FALSE)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Group Has Hawaii EE</label>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
